<div class="page-header">
  <h1>Upload Assignments</h1>
</div>

<div class="row">
  <div class="col-md-6">
    <%= form_tag :assignment_upload, multipart: true do %>
      <div class="form-group">
        <%= file_field_tag :file, class: "form-control" %>
      </div>
      <%= submit_tag 'Upload', class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
