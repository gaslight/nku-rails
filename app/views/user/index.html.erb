<h1>Welcome to Bueller Student Management!</h1>
<%= link_to "Home Page", controller: "user" %> 
<%= link_to 'New Student', new_user_path %>
<table>
  <tr>
    <th>Name</th>
    <th>Nickname</th>
    <th>Email</th>
    <th>Image URL </th>
  </tr>
 
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.name %></td>
      <td><%= post.nickname %></td>
      <td><%= post.email %></td>
      <td><%= post.image %></td>
    </tr>
  <% end %>
</table>
